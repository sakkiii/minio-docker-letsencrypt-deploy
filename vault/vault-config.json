{
  "api_addr": "https://127.0.0.1:8200",
  "backend": {
    "file": {
      "path": "/vault/file"
    }
  },
  "ui": true,
  "default_lease_ttl": "168h",
  "max_lease_ttl": "720h",
  "disable_clustering": true,
  "listener": {
    "tcp": {
      "address": "0.0.0.0:8200",
      "tls_disable":  "true",
      "proxy_protocol_behavior": "use_always"
    }
  }
}
